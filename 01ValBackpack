public class valBackpack
{
	public getMax{int m, int[] P, int[] V}
	{
		int dp = new int[m+1];
		Arrays.fill(0);

		for(int i = 0; i < P.length; i++)
		{
			for(int j = m; j >= 1; j--)
			{
				if(j < A[i])
					dp[j] = dp[j];
				else
					dp[j] = Mathmax(dp[j], dp[j - A[i] + V[i]);
			}
		}
		return dp[m];
	} 
}
